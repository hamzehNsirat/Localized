{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": [
        "<node_internals>/**",
        "<node_modules>/**"
      ],
      "program": "${workspaceFolder}/Back-End/app.js", // Update to your entry point file
      "cwd": "${workspaceFolder}",
      "restart": true,
      "console": "integratedTerminal"
    }
  ]
}